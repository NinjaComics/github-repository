<?xml version="1.0"?>
<block>
  <name>SC FDMA Packetheader Parser</name>
  <key>howto_sc_fdma_packet_headerparser_b</key>
  <category>HOWTO</category>
  <import>import howto</import>
  <make>howto.sc_fdma_packet_headerparser_b($header_formatter)</make>
  <param>
    <name>Formatter Object</name>
    <key>header_formatter</key>
    <type>raw</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>header_data</name>
    <type>message</type>
  </source>
</block>
